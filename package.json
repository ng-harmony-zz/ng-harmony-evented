{
  "name": "ng-harmony-powercontroller",
  "version": "0.1.0",
  "description": "Programmatic Eventing in Ctrl-Classes in Angular",
  "main": "dist/es5/system_module.js",
  "dependencies": {
    "ng-harmony": "^0.2.0"
  },
  "devDependencies": {
    "babel": "^5.1.10",
    "js-beautify": "^1.5.5",
    "literate-programming": "^0.8.4"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "literate-programming README.md",
    "beautify": "sed -i 's/^\\s*//' ./dist/raw/ng_harmony_powercontroller.js && js-beautify -m 1 -s 4 -l 0 -p ./dist/raw/ng_harmony_powercontroller.js > ./dist/esnext/ng_harmony_powercontroller.js",
    "transpile_amd": "babel --modules amd -e 1 -s -o ./dist/es5/amd_module.js ./dist/esnext/ng_harmony_powercontroller.js",
    "transpile_common": "babel --modules common -e 1 -s -o ./dist/es5/common_module.js ./dist/esnext/ng_harmony_powercontroller.js",
    "transpile_system": "babel --modules system -e 1 -s -o ./dist/es5/system_module.js ./dist/esnext/ng_harmony_powercontroller.js",
    "transpile_umd": "babel --modules umd -e 1 -s -o ./dist/es5/umd_module.js ./dist/esnext/ng_harmony_powercontroller.js",
    "all": "npm run build && npm run beautify && npm run transpile_amd && npm run transpile_common && npm run transpile_system && npm run transpile_umd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joehannes/ng-harmony-powercontroller.git"
  },
  "keywords": [
    "angular",
    "ng",
    "harmony",
    "es6",
    "class",
    "clazz",
    "klass",
    "OO",
    "object-oriented",
    "library",
    "collection"
  ],
  "author": "joehannes",
  "email": "johannes.neugschwentner@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joehannes/ng-harmony-powercontroller/issues"
  },
  "homepage": "https://github.com/ng-harmony/ng-harmony-powercontroller"
}
